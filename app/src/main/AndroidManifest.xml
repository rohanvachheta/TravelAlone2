<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lenovo.travelalone">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".DetailActivity" />
        <activity android:name=".Profileofboth" />
        <activity android:name=".Dhruvactivity" />

        <service android:name=".MessegingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <meta-data
            android:name="com.google.firebase.messaging.default_notification_icon"
            android:resource="@drawable/ic_stat_name" />
        <meta-data
            android:name="com.google.firebase.messaging.default_notification_color"
            android:resource="@color/colorAccent" />

        <activity android:name=".ProfileView" />
        <activity android:name=".chat.Main2acti" />
        <activity android:name=".news.Main3news" />
        <activity android:name=".news.Add_info" />
        <activity android:name=".news.AddByRetro" />
        <activity android:name=".news.Contact" />
        <activity android:name=".news.Viewnews" />
        <activity android:name=".FinalActivity">

        </activity>
        <activity android:name=".Admin"></activity>
    </application>

</manifest>